<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <title>My Page</title>
  </head>
  <body>
    <table>
      <thead>
        <tr>
          <th>Column1</th>
          <th>Column2</th>
          <th>Column3</th>
        </tr>
      </thead>
      <tbody>
        <% rows.forEach(function(row) { %>
        <tr>
          <td><%= row.problem %></td>
          <td><%= row.action %></td>
          <td><%= row.status %></td>
        </tr>
        <% }); %>
      </tbody>
    </table>
  </body>
</html>
